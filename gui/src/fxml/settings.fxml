<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.jessevogel.synthesizer.gui.windows.ControllerWindowSettings">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" styleClass="settings-list">
               <children>
                  <Button fx:id="buttonMIDIDevices" onAction="#clickMIDIDevices" mnemonicParsing="false" text="MIDI devices" />
                  <Button fx:id="buttonAudioDevices" onAction="#clickAudioDevices" mnemonicParsing="false" text="Audio devices" />
                  <Button mnemonicParsing="false" text="Options" />
               </children>
            </VBox>
            <Pane fx:id="content" styleClass="settings-content" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@../css/style.css" />
      <URL value="@../css/settings.css" />
   </stylesheets>
</VBox>
